{
  "name": "bot-data-service",
  "version": "1.0.0",
  "private": true,
  "description": "Data synchronization service for TeAlquilamos WhatsApp Bot - handles multiple data sources (Beds24, Whapi, etc.)",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "start": "node dist/main.js",
    "prestart": "npx prisma generate --schema ../prisma/schema.prisma",
    "build": "npx tsc -p tsconfig.json",
    "postinstall": "npx prisma generate --schema ../prisma/schema.prisma",
    "db:generate": "prisma generate --schema ../prisma/schema.prisma",
    "db:migrate": "prisma migrate deploy --schema ../prisma/schema.prisma",
    "db:studio": "prisma studio --schema ../prisma/schema.prisma",
    "test": "vitest --config vitest.config.ts",
    "backfill": "tsx scripts/backfill.ts",
    "sync:cancelled": "tsx scripts/backfill.ts -- --type cancelled",
    "sync:leads": "tsx scripts/backfill.ts -- --type leads",
    "monitor": "tsx scripts/monitor.ts",
    "monitor:continuous": "tsx scripts/monitor.ts -- --continuous",
    "monitor:json": "tsx scripts/monitor.ts -- --format json",
    "bulk-update": "tsx scripts/bulk-update-bookings.ts",
    "bulk-update:dry": "tsx scripts/bulk-update-bookings.ts -- --dry-run",
    "detect:missing": "tsx scripts/detect-missing-bookings.ts",
    "sync:manual": "tsx scripts/manual-sync-bookings.ts",
    "update:messages": "tsx scripts/update-all-messages.ts",
    "update:messages:specific": "tsx scripts/update-all-messages.ts",
    "test:railway-token": "tsx scripts/test-railway-token.ts",
    "create:test-booking": "tsx scripts/create-test-booking.ts",
    "sync:all-bookings": "node dist/scripts/sync-all-bookings.js",
    "sync:modified": "node dist/scripts/update-modified-bookings.js",
    "sync:quick": "node dist/scripts/update-modified-bookings.js 2",
    "sync:complete": "node dist/scripts/sync-complete-period.js",
    "sync:optimized": "node dist/scripts/sync-optimized.js",
    "sync:smart": "node dist/scripts/sync-with-rate-limit.js",
    "typecheck": "npx tsc --noEmit",
    "test-build": "npm run typecheck && npm run build"
  },
  "dependencies": {
    "@bull-board/express": "^6.12.0",
    "@prisma/client": "^6.15.0",
    "axios": "^1.7.2",
    "bullmq": "^5.57.0",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "express-prom-bundle": "^8.0.0",
    "ioredis": "^5.7.0",
    "pino": "^9.4.0",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.14.0",
    "prom-client": "^15.1.3",
    "shx": "^0.4.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.5.4",
    "yargs": "^18.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "tsx": "^4.20.4",
    "vitest": "^2.0.5"
  }
}
